# 開發環境配置

# 資料庫配置
DATABASE_URL=sqlite://lifeup.db

# 伺服器配置
SERVER_HOST=127.0.0.1
SERVER_PORT=8080

# 應用程式配置
ENVIRONMENT=development
RUST_LOG=debug

# AI 服務配置
# 選擇 AI 提供商: "OpenAI" 或 "OpenRouter"
API_OPTION=OpenRouter

# OpenAI 配置 (當 API_OPTION=OpenAI 時需要)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# OpenRouter 配置 (當 API_OPTION=OpenRouter 時需要)
OPENROUTER_API_KEY=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
OPENROUTER_MODEL=qwen/qwen3-8b

# AI 模型等級配置 (Small/Fast/Normal/Think/Background)
# Small - 超輕量，適合極簡單的文字處理、格式轉換、基礎驗證
AI_MODEL_SMALL=google/gemma-3-4b-it

# Fast - 快速回應，適合簡單對話、快速回覆、任務預覽
AI_MODEL_FAST=qwen/qwen3-8b

# Normal - 標準推理，適合一般任務生成、成就生成、專家匹配
AI_MODEL_NORMAL=google/gemma-3n-e4b-it

# Think - 深度推理，適合複雜規劃、專家分析(方向/目標/資源)、子任務生成
AI_MODEL_THINK=openai/gpt-oss-120b

# Background - 背景處理，適合非即時的大量數據分析、批次處理、深度研究
AI_MODEL_BACKGROUND=google/gemma-3-4b-it
